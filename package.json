{
    "name": "configjs",
    "version": "0.1.0",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "dev": "turbo run dev",
        "build": "turbo run build",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "changeset": "changeset",
        "version-packages": "changeset version",
        "release": "turbo run build && changeset publish"
    },
    "devDependencies": {
        "@changesets/cli": "^2.22.0",
        "eslint": "^7.32.0",
        "prettier": "^2.8.4",
        "turbo": "latest"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "dependencies": {},
    "packageManager": "pnpm@7.27.0"
}
